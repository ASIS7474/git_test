CASE
    WHEN extract(month, current_date) = 1 THEN 'December'
    ELSE 
        substring(
            'JanuaryFebruaryMarchAprilMayJuneJulyAugustSeptemberOctoberNovember', 
            (extract(month, add_months(current_date, -1)) - 1) * 9 + 1, 
            9
        )
END || ' ' || extract(year, add_months(current_date, -1))


cast(
    add_months(current_date, -1) as char(7)
)

